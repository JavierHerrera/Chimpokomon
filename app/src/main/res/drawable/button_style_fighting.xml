<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="1px">
        <shape android:shape="rectangle">
            <solid android:color="#cacaca" />
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:left=".5px" android:right="1.5px" android:top="1px" android:bottom="1.5px">
        <shape android:shape="rectangle">
            <solid android:color="#a9a9a9" />
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item android:left="1px" android:right="2px" android:top="1px" android:bottom="2px">
        <shape android:shape="rectangle">
            <solid android:color="#EF9A9A" />
            <corners android:radius="3dp"/>
        </shape>
    </item>
</layer-list>